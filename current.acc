type Point struct
    int x
    int y
end

func print(int number) void
    printf("%d\n", number)
end

func add(int a, int b) int
    return a + b
end

func incr(@int a) void
	$a = $a + 1
end

var Point p
p.x = 1
p.y = 6
var @Point q
q = @p
incr(@(q.x))
incr(@(p.x))
